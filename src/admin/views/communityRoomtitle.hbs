   <body>
      <section class="trsuing">
         <div class="container">
            <a href="/">
               <div class="logo">
                  <img src="../../img/Truso_logo.png">
               </div>
            </a>
         </div>
         {{#if data}}
      </section>
      <section class="untree_co-hero1">
          <div class="main-content">
       <div class="container">
           <div class="card" style="margin-top: 2% !important">
               <header class="text-center text-white" style="font-size: 30px;letter-spacing: 2px;font-weight: bold;">Community Room</header>
                <br>
            <div class="row pl-3">
               <div class="col-lg-12">
                    <div class="headsss">
                        <form action="">
                            <lable class="text-white">
                                        <h5>Select Room Title </h5>
                                    </lable>
                            <select name="category" class="form-control" id="roomUid">
                                <option value="">Select</option>
                                    {{#each data}}
                                        <option value="{{{this.uid}}}">{{this.title}}</option>
                                    {{/each}}
                            </select>
                        </form>
                        <br>
                    </div>
               </div>
           </div>
           <div id="dataappend">

           </div>
      </div>
       </div>
      </section>
      {{/if}}
      <footer id="footer" class="footer-area">
         <div class="footer-widget">
            <div class="container">
               <div class="row">
                  <div class="col-lg-4 col-sm-6">
