                        <li><a href="https://twitter.com/jointruso/"><i class="fab fa-twitter" aria-hidden="true"></i></a></li>
                        <li><a href="https://www.instagram.com/jointruso/"><i class="fab fa-instagram" aria-hidden="true"></i></li>
                        </ul>
                     </div>
                  </div>
               </div>
               <div class="row">
                  <div class="col-md-12">
                     <div class="rights-rsvd">
                        <p>All Right Reserved @ Copyright 2021</p>
                     </div>
                  </div>
               </div>
            </div>
         </div>
      </footer>
   </body>
   
   <script>
    $(function(){
        $("#roomUid").change(function(e){
            let roomUid = $(this).val();
            $('#dataappend div').remove();
            $('#dataappend p').remove();
            $('#dataappend h2').remove();
            $('#dataappend header').remove();
            $.ajax({
                url:`/admin/Show_communityRoom?roomUid=${roomUid}`,
                type:'GET', 
                success:function(data){
                        $('#dataappend').append(`<div class="card sdfasda" style="margin-top: 2% !important" >
            <header class="text-center text-white" style="font-size: 30px;letter-spacing: 2px;font-weight: bold;">Community Room Details</header>
            <br>
            <div class="row pl-3">
                <div class="col-lg-4">
                    <div class="headsss">
                        <h2 class="font_2">title :</h2>
                        <p class="font_8">${data.title}</p>
                        <h2 class="font_2">startTime :</h2>
                        <p class="font_8"> ${data.startTime} </p>
                        <h2 class="font_2">endTime :</h2>
                        <p class="font_8">${data.endTime}</p>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="headsss">
                        <h2 class="font_2">hoster Name :</h2>
                        <p class="font_8"> ${data.hosterID} </p>
                        <h2 class="font_2">description :</h2>
                        <p class="font_8"> ${data.description} </p>
                        <h2 class="font_2">hands Name :</h2>
                        <p class="font_8"> ${data.hands} </p>
                        
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="headsss">
                        <h2 class="font_2">thumbImageURL</h2>
                        <p class="font_8"> 
                            <img src="${data.thumbImageURL}" class="rounded d-block img-thumbnail" style="width: 20%;" alt="Responsive image">
                            </p>
                        <h2 class="font_2">bgImageURL</h2>
                        <p class="font_8">
                            <img src="${data.bgImageURL}" class="rounded d-block img-thumbnail" style="width: 20%;" alt="Responsive image">
                            </p>
                        <h2 class="font_2">hosters Name :</h2>
                        <p class="font_8">${data.hosters}</p>
                    </div>
                </div>
           </div>
           <div class="row pl-3">
                <div class="col-lg-4">
                    <div class="headsss">
                        <h2 class="font_2">idArr Name :</h2>
                        <p class="font_8"> ${data.idArr} </p>
                        <h2 class="font_2">audiences Name :</h2>
                        <p class="font_8">${data.audiences}</p>
                        <h2 class="font_2">inviters Name :</h2>
                        <p class="font_8">${data.inviters}</p>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="headsss">
                        <h2 class="font_2">allParticipants Name :</h2>
                        <p class="font_8">${data.allParticipants}</p>
                        <h2 class="font_2">allParticipantIds Name :</h2>
                        <p class="font_8">${data.allParticipantIds}</p>
                    </div>
                </div>
                <div class="col-lg-4">
                    <br>
                    <br>
                    <br>
                    <h2 class="font_2"> Delete Profile Image <a class="btn btn-sm btn-outline-danger text-white" title="Delete profile image" href="/admin/deleteprofileimageC/${data.uid}" style="text-decoration: none;"><i class="fas fa-trash"></i></a></h2>
                    <h2 class="font_2"> Delete bg Image <a class="btn btn-sm btn-outline-danger text-white" title="Delete bg image" href="/admin/deletebgimageC/${data.uid}" style="text-decoration: none;"><i class="fas fa-trash"></i></a></h2>
                    <div class="headsss">
                        <h2 class="font_2">  <a class="btn btn-outline-danger text-white" title="Delete Community Room" href="/admin/deletecommunityroom/${data.uid}" style="text-decoration: none;"><i class="fas fa-trash"></i> &nbsp;&nbsp;Delete Community Room</a></h2>
                    </div>
                </div>
           </div>
           </div>`
           );
                }
            });
        });
    });
</script>
</html>
